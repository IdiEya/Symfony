    <script>
       let currentSortColumn = null;
let currentSortDirection = 'asc';

function sortTable(n) {
    let table = document.getElementById("coachsTable");
    let rows = Array.from(table.rows).slice(1); // Exclure l'en-tête
    let dir = currentSortColumn === n && currentSortDirection === 'asc' ? 'desc' : 'asc';
    let arrow = document.getElementById(getColumnArrowId(n));

    // Trier les lignes en fonction de la colonne sélectionnée
    rows.sort((rowA, rowB) => {
        let cellA = rowA.cells[n].innerText.trim().toLowerCase();
        let cellB = rowB.cells[n].innerText.trim().toLowerCase();

        if (dir === 'asc') {
            return cellA < cellB ? -1 : cellA > cellB ? 1 : 0;
        } else {
            return cellA > cellB ? -1 : cellA < cellB ? 1 : 0;
        }
    });

    // Réinsérer les lignes triées dans le tableau
    rows.forEach(row => table.appendChild(row));

    // Mettre à jour l'état du tri
    currentSortColumn = n;
    currentSortDirection = dir;

    // Mettre à jour les flèches de tri
    updateArrows();
    arrow.innerText = dir === 'asc' ? '↑' : '↓';
}


function getColumnArrowId(columnIndex) {
    const columnNames = ['nom', 'prenom', 'specialite', 'telephone', 'email'];
    return columnNames[columnIndex] + '-arrow';
}

function updateArrows() {
    const arrows = ['nom', 'prenom', 'specialite', 'telephone', 'email'];
    arrows.forEach(col => {
        let arrow = document.getElementById(col + '-arrow');
        arrow.innerText = '';
    });
}

    </script>
    
    
